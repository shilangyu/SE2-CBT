{
    "name": "cbt-web",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:gh-pages": "tsc && vite build --base=/SE2-CBT/",
        "preview": "vite preview",
        "lint": "prettier --check . --ignore-path .gitignore && eslint ./src --ext .jsx,.js,.ts,.tsx --ignore-path .gitignore",
        "lint:fix": "prettier --write . --ignore-path .gitignore && eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path .gitignore",
        "test": "jest --passWithNoTests"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.6.2",
        "@mui/material": "^5.6.0",
        "notistack": "^2.0.3",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router-dom": "^6.3.0",
        "ts-jest": "^27.1.4",
        "ts-routes": "^2.0.0",
        "zustand": "^3.7.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@types/jest": "^27.4.1",
        "@types/react": "^18.0.1",
        "@types/react-dom": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "@vitejs/plugin-react": "^1.3.0",
        "eslint": "^8.13.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "jest": "^27.5.1",
        "prettier": "^2.6.2",
        "typescript": "^4.6.2",
        "vite": "^2.9.1"
    }
}
